version = "1.0.0"

measurements:
(
    {
        table_name: "system_proc",
        entity_name: "proc",
        fields:
        (
            {
                description: "id of process",
                type: "key",
                name: "tgid",
            },
            {
                description: "process group id",
                type: "label",
                name: "pgid",
            },
            {
                description: "parent id of process",
                type: "label",
                name: "ppid",
            },
            {
                description: "comm of the process",
                type: "label",
                name: "comm",
            },
            {
                description: "cmdline of the process",
                type: "label",
                name: "cmdline",
            },
            {
                description: "ID of container",
                type: "label",
                name: "container_id",
            },
            {
                description: "fd counts of the process",
                type: "gauge",
                name: "fd_count",
            },
            {
                description: "percent of remaining fd resource(%)",
                type: "gauge",
                name: "fd_free_per",
            },
            {
                description: "bytes of proc system call I/O read operation",
                type: "counter",
                name: "proc_rchar_bytes",
            },
            {
                description: "bytes of proc system call I/O write operation",
                type: "counter",
                name: "proc_wchar_bytes",
            },
            {
                description: "count of proc system call I/O read operation",
                type: "counter",
                name: "proc_syscr_count",
            },
            {
                description: "count of proc system call I/O write operation",
                type: "counter",
                name: "proc_syscw_count",
            },
            {
                description: "bytes of proc read operation from disk",
                type: "counter",
                name: "proc_read_bytes",
            },
            {
                description: "bytes written to disk by proc (expected)",
                type: "counter",
                name: "proc_write_bytes",
            },
            {
                description: "bytes written to disk cancelled by proc",
                type: "counter",
                name: "proc_cancelled_write_bytes",
            },
            {
                description: "size of clean pages in shared memory(kB)",
                type: "counter",
                name: "proc_shared_clean_size",
            },
            {
                description: "size of dirty pages in shared memory(kB)",
                type: "counter",
                name: "proc_shared_dirty_size",
            },
            {
                description: "size of clean pages in private memory(kB)",
                type: "counter",
                name: "proc_private_clean_size",
            },
            {
                description: "size of dirty pages in private memory(kB)",
                type: "counter",
                name: "proc_private_dirty_size",
            },
            {
                description: "size of memory currently marked as referenced(kB)",
                type: "counter",
                name: "proc_referenced_size",
            },
            {
                description: "size of memory not immediately freed(kB)",
                type: "counter",
                name: "proc_lazyfree_size",
            },
            {
                description: "size of data present in proc swap partition(kB)",
                type: "counter",
                name: "proc_swap_data_size",
            },
            {
                description: "size of proc swap physical partition(kB)",
                type: "counter",
                name: "proc_swap_data_pss_size",
            },
            {
                description: "time of proc runs in userspace",
                type: "counter",
                name: "proc_utime_jiffies",
            },
            {
                description: "time of proc runs in kernelspace",
                type: "counter",
                name: "proc_stime_jiffies",
            },
            {
                description: "count of proc cirtical pagefault",
                type: "counter",
                name: "proc_minor_pagefault_count",
            },
            {
                description: "count of proc weak pagefault",
                type: "counter",
                name: "proc_major_pagefault_count",
            },
            {
                description: "byte of proc's virtual address space(B)",
                type: "counter",
                name: "proc_vm_size",
            },
            {
                description: "size of proc's physical address space(page)",
                type: "counter",
                name: "proc_pm_size",
            }
        )
    }
)

